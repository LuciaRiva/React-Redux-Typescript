{
    "plugins": [],
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules|documentation)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./documentation",
        "recurse": true,
        "verbose":true
    }
}